  shorttitle = {The Potential Diagnostic Value of Extracellular Vesicle {{miRNA}} for Human Non-Small Cell Lung Cancer},
  author = {Huang, Hairong and Zhu, Jinyuan and Lin, Yong and Zhang, Zhexiao and Liu, Jie and Wang, Chenfei and Wu, Hongfu and Zou, Tangbin},
