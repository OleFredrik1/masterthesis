  author = {Kryczka, Jolanta and {Migdalska-S{\k{e}}k}, Monika and Kordiak, Jacek and Kisza{\l}kiewicz, Justyna M. and {Pastuszak-Lewandoska}, Dorota and Antczak, Adam and {Brzezia{\'n}ska-Lasota}, Ewa},
