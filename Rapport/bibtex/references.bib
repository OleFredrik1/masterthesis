@article{Ma2011,
  title = {An {{Integrated Analysis}} of {{miRNA}} and {{mRNA Expressions}} in {{Non}}-{{Small Cell Lung Cancers}}},
  author = {Ma, Lina and Huang, Yanyan and Zhu, Wangyu and Zhou, Shiquan and Zhou, Jihang and Zeng, Fang and Liu, Xiaoguang and Zhang, Yongkui and Yu, Jun},
